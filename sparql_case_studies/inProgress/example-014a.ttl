@prefix ex: <https://purl.org/emi#examples> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ex:014a a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "## List of possible natural locations of plants and their interactions, which produce Bahamaolide A,  (wd:Q75068439) which is an antifungal (https://doi.org/10.1021/np3001915)."@en ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """
		PREFIX wd: <http://www.wikidata.org/entity/>
		PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
		PREFIX enpkg: <https://enpkg.commons-lab.org/kg/>
		PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
		PREFIX wdt: <http://www.wikidata.org/prop/direct/>
		PREFIX emi: <https://purl.org/emi#>
		PREFIX emiBoxOrg: <https://purl.org/emi/abox#ORGANISM->
		PREFIX sosa: <http://www.w3.org/ns/sosa/>
		PREFIX dcterms: <http://purl.org/dc/terms/>
		PREFIX wgs: <http://www.w3.org/2003/01/geo/wgs84_pos#>
		PREFIX prov: <http://www.w3.org/ns/prov#>
		SELECT DISTINCT ?plantNameURI ?plantWD ?loc ?lats ?longs WHERE {
		        { SELECT DISTINCT ?plantWD WHERE {				#retrieve metabolite data
				?material sosa:hasSample ?extract ;
		        		  sosa:isSampleOf ?organe .
			    	?organe emi:inTaxon ?plantWD .				#filter metabolite data which is found in wikidata-ids wdx	
			    	FILTER(?plantWD = ?source_wdx || ?plantWD = ?target_wdx)				#ensure that plant-wd is either from source or target
			    	?extract sosa:isFeatureOfInterestOf ?lcms .
			    	?lcms sosa:hasResult ?feature_list .
				?feature_list emi:hasLCMSFeature ?feature .
				?feature emi:hasAnnotation ?sirius_annotation .
			    	?sirius_annotation a emi:StructuralAnnotation ;
			       		emi:hasChemicalStructure ?ik2d .
			    	?ik2d emi:hasSMILES ?smiles ;
			    	      emi:isInChIKey2DOf ?ik .
				?ik emi:isInChIKeyOf ?wd_chem .					#retrieve wikidata-ids for metabolites
				VALUES ?wd_chem { wd:Q75068439 }				#filter wikidata_id of metabolites matches wd:Q75068439
			
			}} UNION							#union with data from lotus (integrated in wikidata)
			{ SERVICE <https://qlever.cs.uni-freiburg.de/api/wikidata> {
				?wd_chem wdt:P235 ?ik ;
	    				 wdt:P703 ?plantWD .
			 	VALUES ?wd_chem { wd:Q75068439 }
			 	FILTER(?plantWD = ?source_wdx || ?plantWD = ?target_wdx)				#ensure that plant-wd is either from source or target
			}}
			?plantWD sosa:isSampleOf ?plantName .				#retrieve scientific name of the plant
			BIND(REPLACE(STR(?plantName), "%20", " ") AS ?plantNameX)	#remove percent encodings from the source/target names
			BIND(IRI(?plantNameX) AS ?plantNameURI)
			
			 	
			?intxn emi:hasSource ?source ;					#retrieve interaction-pairs
			       emi:hasTarget ?target .
			?source emi:inTaxon ?source_wdx .				#retrieve wikidata-id of source
			?target emi:inTaxon ?target_wdx .				#retrieve wikidata-id of target
			OPTIONAL {
				?intxn prov:atLocation ?loc ;				#optionally retrieve the location of the interaction, and the latitude/longitude
				       wgs:lat ?lats ;					
				       wgs:long ?longs .
			}
			
			
		}
""" ;
    schema:target <https://purl.org/emi> .
